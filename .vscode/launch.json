    "version": "0.2.0",
    "configurations": [
        {
            "name": "Java",
            "type": "java",
            "request": "launch",
            "stopOnEntry": true,
            "jdkPath": "${env:JAVA_HOME}/bin",
            "cwd": "${fileDirname}",
            "startupClass": "${fileBasenameNoExtension}",
            "classpath": [
                ".",
                "${fileDirname}"
            ]
        },
        {
            "name": "Java Console App",
            "type": "java",
            "request": "launch",
            "stopOnEntry": true,
            "jdkPath": "${env:JAVA_HOME}/bin",
            "cwd": "${fileDirname}",
            "startupClass": "${fileBasenameNoExtension}",
            "classpath": [
                ".",
                "${fileDirname}"
            ],
            "externalConsole": true
        },
        {
            "type": "java",
            "name": "Launch Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Launch DBSessionTest",
            "request": "launch",
            "mainClass": "org.chaostocosmos.net.porta.DBSessionTest",
            "projectName": "porta"
        },
        {
            "type": "java",
            "name": "Launch ManagementServerTest",
            "request": "launch",
            "mainClass": "org.chaostocosmos.net.porta.ManagementServerTest",
            "projectName": "porta"
        },
        {
            "type": "java",
            "name": "Launch PortaMainTest",
            "request": "launch",
            "mainClass": "org.chaostocosmos.net.porta.PortaMainTest",
            "projectName": "porta"
        },
        {
            "type": "java",
            "name": "Launch ProxySessionTest",
            "request": "launch",
            "mainClass": "org.chaostocosmos.net.porta.ProxySessionTest",
